<?xml version="1.0"  encoding="ISO-8859-1"?>
<!DOCTYPE Configure PUBLIC "-//Mort Bay Consulting//DTD Configure//EN" "http://www.eclipse.org/jetty/configure.dtd">
 
<Configure class="org.eclipse.jetty.webapp.WebAppContext">

	<New class="org.eclipse.jetty.plus.jndi.EnvEntry">
		<Arg></Arg>
		<Arg>logName</Arg>
		<Arg type="java.lang.String">fsi-customer</Arg>
		<Arg type="boolean">false</Arg>
	</New>
	<New class="org.eclipse.jetty.plus.jndi.EnvEntry">
		<Arg></Arg>
		<Arg>logFilePrefix</Arg>
		<Arg type="java.lang.String">D:\\gitprojects\\fsi-customer\\target\\log_fsi-customer</Arg>
		<Arg type="boolean">false</Arg>
	</New>
	<New class="org.eclipse.jetty.plus.jndi.EnvEntry">
		<Arg></Arg>
		<Arg>logLevel</Arg>
		<Arg type="java.lang.String">INFO</Arg>
		<Arg type="boolean">false</Arg>
	</New>	

	
	<New class="org.eclipse.jetty.plus.jndi.EnvEntry">
		<Arg></Arg>
		<Arg>logFileSize</Arg>
		<Arg type="java.lang.String">200000</Arg>
		<Arg type="boolean">false</Arg>
	</New>	

	<New class="org.eclipse.jetty.plus.jndi.EnvEntry">
		<Arg></Arg>
		<Arg>logFilesCount</Arg>
		<Arg type="java.lang.String">10</Arg>
		<Arg type="boolean">false</Arg>
	</New>	
	
	<New class="org.eclipse.jetty.plus.jndi.EnvEntry">
		<Arg></Arg>
		<Arg>configVersion</Arg>
		<Arg type="java.lang.String">production</Arg>
		<Arg type="boolean">false</Arg>
	</New>	

	<New class="org.eclipse.jetty.plus.jndi.EnvEntry">
		<Arg></Arg>
		<Arg>applicationBaseURL</Arg>
		<Arg type="java.lang.String">http://localhost:8080/fsi-customer/</Arg>
		<Arg type="boolean">false</Arg>
	</New>	

	<New class="org.eclipse.jetty.plus.jndi.EnvEntry">
		<Arg></Arg>
		<Arg>resourceRootURL</Arg>
		<Arg type="java.lang.String">/fsi-customer/resources/</Arg>
		<Arg type="boolean">false</Arg>
	</New>	
	
	<New class="org.eclipse.jetty.plus.jndi.EnvEntry">
		<Arg></Arg>
		<Arg>protectedResourceRootURL</Arg>
		<Arg type="java.lang.String">/fsi-customer/protected/</Arg>
		<Arg type="boolean">false</Arg>
	</New>	

	<New class="org.eclipse.jetty.plus.jndi.EnvEntry">
		<Arg></Arg>
		<Arg>resourceDiskRootFolder</Arg>
		<Arg type="java.lang.String">D:\\gitprojects\\fsi-customer\\src\\main\\webapp\\resources</Arg>
		<Arg type="boolean">false</Arg>
	</New>	
	
	<New class="org.eclipse.jetty.plus.jndi.EnvEntry">
		<Arg></Arg>
		<Arg>protectedResourceDiskRootFolder</Arg>
		<Arg type="java.lang.String">D:\\gitprojects\\fsi-customer\\src\\main\\webapp\\protected</Arg>
		<Arg type="boolean">false</Arg>
	</New>	

	<New class="org.eclipse.jetty.plus.jndi.EnvEntry">
		<Arg></Arg>
		<Arg>indexDiskRootFolder</Arg>
		<Arg type="java.lang.String">D:\\gitprojects\\fsi-customer\\target\\temp\\indexdir</Arg>
		<Arg type="boolean">false</Arg>
	</New>
	
	<New id="portDataSource" class="org.eclipse.jetty.plus.jndi.Resource">
		<Arg>java:comp/env/jdbc/portDataSource</Arg>
		<Arg>
			<New class="org.apache.commons.dbcp.BasicDataSource">
				<Set name="driverClassName">org.postgresql.Driver</Set>
				<Set name="url">jdbc:postgresql://localhost:5432/fsi-customerPort</Set>
				<Set name="username">agile</Set>
				<Set name="password">agile</Set>
			</New>
		</Arg>
	</New>
	
	<New id="servDataSource" class="org.eclipse.jetty.plus.jndi.Resource">
		<Arg>java:comp/env/jdbc/servDataSource</Arg>
		<Arg>
			<New class="org.apache.commons.dbcp.BasicDataSource">
				<Set name="driverClassName">org.postgresql.Driver</Set>
				<Set name="url">jdbc:postgresql://localhost:5432/fsi-customerServ</Set>
				<Set name="username">agile</Set>
				<Set name="password">agile</Set>
			</New>
		</Arg>
	</New>

</Configure>
